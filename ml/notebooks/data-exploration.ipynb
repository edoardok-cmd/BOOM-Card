    "# Function to create sample data for demonstration\n",
    "def create_sample_data(dataset_name):\n",
    "    \"\"\"Create sample data for demonstration purposes\"\"\"\n",
    "    np.random.seed(42)\n",
    "    \n",
    "    if dataset_name == 'users':\n",
    "        n = 10000\n",
    "        return pd.DataFrame({\n",
    "            'user_id': range(1, n+1),\n",
    "            'email': [f'user{i}@example.com' for i in range(1, n+1)],\n",
    "            'subscription_type': np.random.choice(['free', 'basic', 'premium'], n, p=[0.3, 0.5, 0.2]),\n",
    "            'subscription_status': np.random.choice(['active', 'cancelled', 'expired'], n, p=[0.7, 0.2, 0.1]),\n",
    "            'created_at': pd.date_range(end=datetime.now(), periods=n, freq='H'),\n",
    "            'last_active': pd.date_range(end=datetime.now(), periods=n, freq='2H'),\n",
    "            'city': np.random.choice(['Sofia', 'Plovdiv', 'Varna', 'Burgas', 'Ruse'], n),\n",
    "            'country': 'Bulgaria',\n",
    "            'total_savings': np.random.exponential(50, n),\n",
    "            'transaction_count': np.random.poisson(10, n),\n",
    "            'favorite_categories': np.random.choice(['restaurants', 'hotels', 'entertainment', 'spa'], n)\n",
    "        })\n",
    "    \n",
    "    elif dataset_name == 'partners':\n",
    "        n = 500\n",
    "        return pd.DataFrame({\n",
    "            'partner_id': range(1, n+1),\n",
    "            'business_name': [f'Business {i}' for i in range(1, n+1)],\n",
    "            'category': np.random.choice(['Food & Drink', 'Accommodation', 'Entertainment', 'Services'], n),\n",
    "            'subcategory': np.random.choice(['Restaurant', 'Hotel', 'Spa', 'Bar', 'Club'], n),\n",
    "            'discount_percentage': np.random.randint(5, 31, n),\n",
    "            'location_lat': np.random.uniform(42.5, 43.5, n),\n",
    "            'location_lng': np.random.uniform(23.0, 24.0, n),\n",
    "            'city': np.random.choice(['Sofia', 'Plovdiv', 'Varna', 'Burgas'], n),\n",
    "            'rating': np.random.uniform(3.5, 5.0, n),\n",
    "            'review_count': np.random.poisson(50, n),\n",
    "            'active_since': pd.date_range(end=datetime.now(), periods=n, freq='D'),\n",
    "            'total_transactions': np.random.poisson(100, n),\n",
    "            'total_revenue_generated': np.random.exponential(5000, n)\n",
    "        })\n",
    "    \n",
    "    elif dataset_name == 'transactions':\n",
    "        n = 50000\n",
    "        return pd.DataFrame({\n",
    "            'transaction_id': range(1, n+1),\n",
    "            'user_id': np.random.randint(1, 10001, n),\n",
    "            'partner_id': np.random.randint(1, 501, n),\n",
    "            'transaction_date': pd.date_range(end=datetime.now(), periods=n, freq='10T'),\n",
    "            'original_amount': np.random.exponential(50, n) + 10,\n",
    "            'discount_amount': lambda: [orig * np.random.uniform(0.05, 0.3) for orig in np.random.exponential(50, n) + 10],\n",
    "            'final_amount': lambda: [orig - disc for orig, disc in zip(np.random.exponential(50, n) + 10, \n",
    "                                                                      [orig * np.random.uniform(0.05, 0.3) for orig in np.random.exponential(50, n) + 10])],\n",
    "            'payment_method': np.random.choice(['card', 'cash', 'mobile'], n, p=[0.6, 0.3, 0.1]),\n",
    "            'device_type': np.random.choice(['mobile', 'pos', 'web'], n, p=[0.5, 0.4, 0.1]),\n",
    "            'qr_scan_time': pd.date_range(end=datetime.now(), periods=n, freq='10T'),\n",
    "            'transaction_complete_time': pd.date_range(end=datetime.now() + timedelta(minutes=5), periods=n, freq='10T')\n",
    "        })\n",
    "    \n",
    "    elif dataset_name == 'subscriptions':\n",
    "        n = 7000\n",
    "        return pd.DataFrame({\n",
    "            'subscription_id': range(1, n+1),\n",
    "            'user_id': np.random.randint(1, 10001, n),\n",
    "            'plan_type': np.random.choice(['free', 'basic', 'premium'], n, p=[0.3, 0.5, 0.2]),\n",
    "            'start_date': pd.date_range(end=datetime.now(), periods=n, freq='D'),\n",
    "            'end_date': pd.date_range(start=datetime.now(), periods=n, freq='D'),\n",
    "            'amount_paid': np.random.choice([0, 9.99, 19.99], n),\n",
    "            'payment_method': np.random.choice(['card', 'paypal', 'bank_transfer'], n, p=[0.7, 0.2, 0.1]),\n",
    "            'auto_renew': np.random.choice([True, False], n, p=[0.6, 0.4]),\n",
    "            'cancellation_date': pd.NaT\n",
    "        })\n",
    "\n",
    "# Create sample data if database connection failed\n",
    "if not engine:\n",
    "    for name in queries.keys():\n",
    "        dfs[name] = create_sample_data(name)\n",
    "        print(f\"üìä Created sample {name}: {len(dfs[name])} records\")"
   ]
