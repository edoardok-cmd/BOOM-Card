    "## 1. Data Loading and Initial Overview"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Load main datasets\n",
    "queries = {\n",
    "    'users': \"\"\"\n",
    "        SELECT \n",
    "            user_id, email, subscription_type, subscription_status,\n",
    "            created_at, last_active, city, country, \n",
    "            total_savings, transaction_count, favorite_categories\n",
    "        FROM users\n",
    "        WHERE created_at >= NOW() - INTERVAL '1 year'\n",
    "    \"\"\",\n",
    "    \n",
    "    'partners': \"\"\"\n",
    "        SELECT \n",
    "            partner_id, business_name, category, subcategory,\n",
    "            discount_percentage, location_lat, location_lng,\n",
    "            city, rating, review_count, active_since,\n",
    "            total_transactions, total_revenue_generated\n",
    "        FROM partners\n",
    "        WHERE is_active = true\n",
    "    \"\"\",\n",
    "    \n",
    "    'transactions': \"\"\"\n",
    "        SELECT \n",
    "            transaction_id, user_id, partner_id, \n",
    "            transaction_date, original_amount, discount_amount,\n",
    "            final_amount, payment_method, device_type,\n",
    "            qr_scan_time, transaction_complete_time\n",
    "        FROM transactions\n",
    "        WHERE transaction_date >= NOW() - INTERVAL '6 months'\n",
    "    \"\"\",\n",
    "    \n",
    "    'subscriptions': \"\"\"\n",
    "        SELECT \n",
    "            subscription_id, user_id, plan_type, \n",
    "            start_date, end_date, amount_paid,\n",
    "            payment_method, auto_renew, cancellation_date\n",
    "        FROM subscriptions\n",
    "    \"\"\"\n",
    "}\n",
    "\n",
    "# Load data into dataframes\n",
    "dfs = {}\n",
    "for name, query in queries.items():\n",
    "    try:\n",
    "        dfs[name] = pd.read_sql(query, engine)\n",
    "        print(f\"‚úÖ Loaded {name}: {len(dfs[name])} records\")\n",
    "    except Exception as e:\n",
    "        print(f\"‚ùå Error loading {name}: {e}\")\n",
    "        # Create sample data for demonstration\n",
    "        dfs[name] = create_sample_data(name)"
   ]
