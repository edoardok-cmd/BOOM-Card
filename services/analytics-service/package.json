{
  "name": "@boomcard/analytics-service",
  "version": "1.0.0",
  "description": "Analytics and reporting microservice for BOOM Card platform",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**' --ext 'ts,json' --exec 'ts-node' src/index.ts",
    "build": "rimraf dist && tsc",
    "test": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration --runInBand",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "typecheck": "tsc --noEmit",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "docker:build": "docker build -t boomcard/analytics-service .",
    "docker:run": "docker run -p 3003:3003 boomcard/analytics-service"
  },
  "keywords": [
    "analytics",
    "reporting",
    "microservice",
    "boomcard",
    "typescript"
  ],
  "author": "BOOM Card Development Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "@elastic/elasticsearch": "^8.13.0",
    "@sentry/node": "^7.109.0",
    "@sentry/tracing": "^7.109.0",
    "amqplib": "^0.10.3",
    "bull": "^4.12.2",
    "clickhouse": "^2.5.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.12.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "node-cron": "^3.0.3",
    "p-queue": "^8.0.1",
    "pg": "^8.11.5",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "prometheus-api-metrics": "^3.2.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.7",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boomcard/analytics-service.git"
  },
  "bugs": {
    "url": "https://github.com/boomcard/analytics-service/issues"
  },
  "homepage": "https://github.com/boomcard/analytics-service#readme"
}
